{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "after": false,
    "afterEach": false,
    "before": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "exports": false,
    "it": false,
    "module": false,
    "require": false,
    "sinon": false
  },
  "parserOptions": {
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalRequire": false
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "json-format"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "indent": [
      "error",
      2
    ],
    "no-underscore-dangle": 0
  }
}
